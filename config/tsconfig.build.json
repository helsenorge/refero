{
  "extends": "../node_modules/@helsenorge/core-build/lib/tsconfig.webpack.json",
  "compilerOptions": {
    "outDir": "../lib",
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "declaration": true,
    "resolveJsonModule": true
  },
  "include": [
    "../src/**/*.tsx",
    "../src/**/*.ts"
  ],
  "exclude": [
    "../src/**/__tests__*",
    "../lib/**/*.*"
  ]
}