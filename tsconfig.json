{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "jsx": "preserve",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": false,
    "newLine": "CRLF",
    "allowUnreachableCode": false,
    "skipLibCheck": true,
    "lib": ["es5", "es2015.core", "es2015.promise", "dom", "scripthost"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "noEmit": true
  }
}
