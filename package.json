{
  "name": "@helsenorge/refero",
  "version": "16.1.0",
  "description": "Refero is a library that uses a fhir r4 schema and creates a interactive form using helsenorge packages.",
  "keywords": [
    "react",
    "fhir",
    "fhir r4",
    "form",
    "typescript",
    "healthcare",
    "helse",
    "redux",
    "Questionnaire"
  ],
  "homepage": "https://github.com/helsenorge/refero#readme",
  "bugs": {
    "url": "https://github.com/helsenorge/refero/issues"
  },
  "author": "helsenorge",
  "license": "MIT",
  "module": "./refero.es.js",
  "./package.json": "./package.json",
  "files": [
    "lib",
    "README.md",
    "CHANGES",
    "LICENSE",
    "package.json",
    ".vite/manifest.json",
    "style.css"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/refero.git"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx src/ --fix",
    "test": "vitest run",
    "test:watch": "vitest dev",
    "test:ui": "vitest --ui",
    "test:preview": "vite preview --outDir html",
    "dev": "vite",
