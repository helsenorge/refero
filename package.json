{
  "name": "@helsenorge/refero",
  "version": "21.2.0",
  "description": "Refero is a library that uses a fhir r4 schema and creates a interactive form using helsenorge packages.",
  "keywords": [
    "react",
    "fhir",
    "fhir r4",
    "form",
    "typescript",
    "healthcare",
    "helse",
    "redux",
    "Questionnaire"
  ],
  "homepage": "https://github.com/helsenorge/refero#readme",
  "bugs": {
    "url": "https://github.com/helsenorge/refero/issues"
  },
  "author": "helsenorge",
  "license": "MIT",
  "module": "./refero.es.js",
  "./package.json": "./package.json",
  "files": [
    "lib",
    "README.md",
    "CHANGES",
    "LICENSE",
    "package.json",
    ".vite/manifest.json",
    "style.css"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helsenorge/refero.git"
  },
  "scripts": {
    "lint": "eslint src/**/* --fix",
    "test": "vitest --silent=true",
    "test:dev": "vitest",
    "test:watch": "vitest dev",
    "test:ui": "vitest --ui",
    "test:preview": "vite preview --outDir html",
    "dev": "vite",
    "dev:preview": "vite -- --host",
    "build:lib": "vite build",
    "tsc:b": "tsc -b",
    "prebuild": "npm run generate:cssdefinitions",
    "prepare": "npm run build:lib",
    "analyze": "vite build --mode analyze",
    "generate:cssdefinitions": "typed-scss-modules src/**/*.module.scss --implementation sass --nameFormat none --exportType default --includePaths node_modules"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "peerDependencies": {
    "@helsenorge/autosuggest": "^36.0.0",
    "@helsenorge/datepicker": "^12.0.0",
    "@helsenorge/designsystem-react": "^12.0.0",
    "@helsenorge/file-upload": "^36.0.0",
    "dompurify": "3.2.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^9.0.0",
    "redux": "^5.0.0"
  },
  "devDependencies": {
    "@helsenorge/autosuggest": "^36.0.0",
    "@helsenorge/datepicker": "^12.0.0",
    "@helsenorge/designsystem-react": "^12.0.0",
    "@helsenorge/file-upload": "^36.0.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@rollup/plugin-replace": "^6.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/fhir": "^0.0.41",
    "@types/node": "^22.0.0",
    "@types/react-dom": "^18.3.0",
    "@types/rollup-plugin-generate-package-json": "^3.2.9",
    "@ungap/structured-clone": "^1.2.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "autoprefixer": "^10.4.19",
    "browserslist": "^4.23.1",
    "core-js": "^3.37.1",
    "cssnano": "^7.0.3",
    "dompurify": "3.2.6",
    "eslint": "^9.0.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "file-loader": "^6.2.0",
    "husky": "^9.0.11",
    "jsdom": "^27.0.0",
    "prettier": "3.5.3",
    "pretty-quick": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resolve": "^1.22.8",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-visualizer": "^6.0.0",
    "sass": "^1.77.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vite-plugin-lib-inject-css": "^2.0.0",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@helsenorge/core-utils": "^36.0.0",
    "@helsenorge/eslint-config": "^36.0.0",
    "@vitest/web-worker": "^3.2.4",
    "date-fns": "^4.1.0",
    "fhirpath": "^4.0.0",
    "marked": "^14.0.0",
    "react-hook-form": "^7.0.0",
    "uuid": "^13.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,css,scss,md,json}": "prettier --write"
  }
}
